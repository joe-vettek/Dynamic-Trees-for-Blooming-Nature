{
    "multipart": [
        {   "when": { "distance": "1|2" },
            "apply": { "model": "dtbloomingnature:block/palm_leaves/fan_palm_frond" }
        },
        {   "when": { "distance": "3" },
            "apply": { "model": "dtbloomingnature:block/palm_leaves/fan_palm_core_top" }
        },
        {   "when": { "distance": "4" },
            "apply": { "model": "dtbloomingnature:block/palm_leaves/fan_palm_core_bottom" }
        },
        {   "when": {"OR":[ { "direction": "0", "distance": "1|2" }, { "distance": "5|6|7" } ]},
            "apply": { "model": "bloomingnature:block/fan_palm_leaves" }
        }
    ]
}